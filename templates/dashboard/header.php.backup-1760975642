    <!-- Loader -->
    
    <!-- Force Hide Loader Script -->
    <script>
    // Hide loader immediately and prevent it from showing
    (function() {
        // Remove loader attribute from HTML
        if (document.querySelector('html')) {
            document.querySelector('html').removeAttribute('loader');
            document.querySelector('html').setAttribute('loader', 'disable');
        }
        
        // Hide loader element
        var loader = document.getElementById('loader');
        if (loader) {
            loader.style.display = 'none';
            loader.classList.add('d-none');
        }
        
        // Clear localStorage loader setting
        localStorage.setItem('loaderEnable', 'false');
        
        // Backup: Force hide after DOM is ready
        if (document.readyState === 'loading') {
            document.addEventListener('DOMContentLoaded', function() {
                var loader = document.getElementById('loader');
                if (loader) {
                    loader.style.display = 'none';
                    loader.classList.add('d-none');
                }
            });
        }
        
        // Extra backup: Force hide after a short delay
        setTimeout(function() {
            var loader = document.getElementById('loader');
            if (loader) {
                loader.style.display = 'none';
                loader.classList.add('d-none');
            }
        }, 100);
    })();
    </script>

